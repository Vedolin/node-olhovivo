{
  "name": "olhovivo",
  "version": "0.0.4",
  "description": "A node.js wrapper for the SPTrans Olho Vivo API.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --require blanket -R mocha-spec-cov-alt test/**/*.test.js",
    "coverage": "mocha --require blanket -R html-cov test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yamadapc/node-olhovivo"
  },
  "keywords": [
    "olhovivo",
    "sptrans",
    "maps",
    "sao",
    "paulo",
    "brasil",
    "api-wrapper",
    "wrapper"
  ],
  "author": "Pedro Tacla Yamada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamadapc/node-olhovivo/issues"
  },
  "homepage": "https://github.com/yamadapc/node-olhovivo",
  "devDependencies": {
    "blanket": "^1.1.6",
    "mocha": "^2.1.0",
    "mocha-spec-cov-alt": "^0.2.3",
    "nock": "^0.57.0",
    "should": "^4.6.0"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "pattern": [
        "lib"
      ],
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80,
        "lcovOutput": "lcov"
      }
    }
  },
  "dependencies": {
    "bluebird": "^2.7.1",
    "superagent": "^0.21.0"
  }
}
